        android:text="@string/type"
        android:textColor="#27394a"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/imageView12"
        tools:ignore="SmallSp" />

    <TextView
