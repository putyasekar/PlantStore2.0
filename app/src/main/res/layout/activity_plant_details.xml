        android:layout_width="154dp"
        android:layout_height="194dp"
        android:layout_marginEnd="16dp"
        android:transitionName="image"
        app:layout_constraintBottom_toBottomOf="@+id/plant_size"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/plant_name"
        app:srcCompat="@drawable/plant3" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/proximanovasemibold"
        android:text="@string/category"
        android:textColor="#27394A"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/plant_name"
        app:layout_constraintTop_toBottomOf="@+id/plant_name" />

    <TextView
        android:id="@+id/plant_category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/proximanovasemibold"
        android:text="@string/enter_category"
        android:textColor="#27394A"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/plant_siz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/proximanovasemibold"
        android:text="@string/size"
        android:textColor="#27394A"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/plant_category"
        app:layout_constraintTop_toBottomOf="@+id/plant_category" />

    <TextView
        android:id="@+id/plant_size"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/proximanovasemibold"
        android:text="@string/small"
        android:textColor="#27394A"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/plant_siz"
        app:layout_constraintTop_toBottomOf="@+id/plant_siz" />

    <TextView
