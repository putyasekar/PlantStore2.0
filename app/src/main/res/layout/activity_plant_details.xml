        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/proximanovasemibold"
        android:lineSpacingExtra="2dp"
        android:text="@string/this_small_green_and_red_succulent_grows_in_the_shape_of_a_star_giving_it_its_christmas_name_yellow_bumps_run_across_the_leaves_and_border_the_edges_it_turns_more_red_when_happily_stressed_christmas_carol_has_red_pink_flowers_that_typically_bloom_in_the_fall"
        android:textColor="#27394A"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/plant_size" />

    <TextView
        android:id="@+id/plant_price2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/proximanovasemibold"
        android:textColor="#27394a"
        android:textSize="24sp"
        android:transitionName="price"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/plant_description"
