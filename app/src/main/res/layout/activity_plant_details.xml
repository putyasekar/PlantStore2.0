        app:layout_constraintBottom_toBottomOf="@+id/plant_price2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/plant_price2"
        app:srcCompat="@drawable/ic_add_to_bag" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:fontFamily="@font/proximanovasemibold"
        android:gravity="center"
        android:text="@string/free_delivery"
        android:textColor="#4DAA94"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/plant_price2"
        app:layout_constraintStart_toEndOf="@+id/plant_price2"
        app:layout_constraintTop_toTopOf="@+id/plant_price2" />

